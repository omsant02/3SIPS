{
  "name": "eliza.how",
  "description": "Project demonstrating how to make a documentation website with elizaOS",
  "version": "1.0.9",
  "type": "module",
  "private": true,
  "main": "dist/agent.js",
  "module": "dist/agent.js",
  "types": "dist/agent.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/agent.d.ts",
        "default": "./dist/agent.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@codemirror/autocomplete": "^6.18.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.1",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/legacy-modes": "^6.4.2",
    "@codemirror/state": "^6.5.0",
    "@codemirror/view": "^6.35.3",
    "@elizaos/cli": "^1.0.9",
    "@elizaos/core": "^1.0.9",
    "@elizaos/plugin-anthropic": "^1.0.3",
    "@elizaos/plugin-groq": "^1.0.4",
    "@elizaos/plugin-openai": "^1.0.4",
    "@elizaos/plugin-sql": "^1.0.9",
    "@hapi/shot": "^6.0.1",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@libsql/client": "^0.14.0",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tailwindcss/postcss": "^4.0.0-beta.1",
    "@tanstack/react-query": "^5.81.5",
    "@types/uuid": "^10.0.0",
    "@uiw/codemirror-theme-github": "4.21.19",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.1",
    "concurrently": "^9.1.2",
    "date-fns": "^4.1.0",
    "eslint-plugin-path-alias": "^2.1.0",
    "evt": "^2.5.9",
    "js-sha1": "^0.7.0",
    "lucide-react": "^0.525.0",
    "markdown-to-jsx": "^7.7.1",
    "marked": "^15.0.4",
    "next": "^15.3.1",
    "next-nprogress-bar": "^2.3.15",
    "next-themes": "^0.4.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.13.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.7.1",
    "uuid": "^11.1.0",
    "viem": "^2.31.4",
    "wagmi": "^2.15.6",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "tsup": "8.4.0",
    "prettier": "3.5.3",
    "vitest": "2.1.5",
    "@vitest/coverage-v8": "2.1.5",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "postcss": "^8",
    "tailwindcss": "^4.0.0-beta.6",
    "typescript": "^5"
  },
  "scripts": {
    "start": "next start -p 4000",
    "dev": "next dev -p 4000",
    "dev:with-agent": "tsup && concurrently \"elizaos dev\" \"next dev -p 4000\"",
    "build": "tsup && next build",
    "build:next-only": "next build",
    "lint": "prettier --write ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  }
}
